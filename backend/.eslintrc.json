{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    // "no-console": 2,
    "no-var": 2,
    "no-self-compare": 2,
    // "camelcase": 2,
    "eqeqeq": 2,
    "no-unneeded-ternary": 2
  }
}
